@model List<WiFiViewModel>
@if (Model != null)
{
	@if (Model.Count > 0)
	{
		<table class="table table-bordered">
			<thead>
				<tr>
					<th>Service Set Identifier</th>
					<th>Hidden Network</th>
					<th>Auto Join</th>
					<th>Disable Captive Network Detection</th>
					<th>Proxy Setup</th>
					<th>Proxy Server</th>
					<th>Proxy Server Port</th>
					<th>Username</th>
					<th>Proxy URL</th>
					<th>Security Type</th>
					<th>Network Type</th>
					<th>Fast Lane Qos Marking</th>
					<th>Enable Qos Marking</th>
					<th>Whitelist Apple audio/video calling</th>
					<th>Whitelist Specific Apps</th>
					<th>Control</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var wifi in Model)
				{
					<tr>
						<td>@wifi.ServiceSetIdentifier</td>
						<td>@wifi.HiddenNetwork</td>
						<td>@wifi.AutoJoin</td>
						<td>@wifi.DisableCaptiveNetworkDetection</td>
						<td>@wifi.ProxySetup</td>
						<td>@wifi.ProxyServer</td>
						<td>@wifi.ProxyServerPort</td>
						<td>@wifi.Username</td>
						<td>@wifi.ProxyURL</td>
						<td>@wifi.SecurityType</td>
						<td>@wifi.NetworkType</td>
						<td>@wifi.FastLaneQosmarking</td>
						<td>@wifi.EnableQos</td>
						<td>@wifi.Whitelist</td>
						<td>@wifi.App_Identifity</td>
						<td>
							<a onclick="fn_EditWiFi('@wifi.Profile_WIFI_ID')">Edit</a>
						</td>
					</tr>
				}
			</tbody>
		</table>
	}
}
